   "source": [
    "query = \"Buy mobile phone buy phone online\"\n",
    "\n",
    "words = query.lower().split()\n",
    "\n",
    "counts = {}\n",
    "for word in words:\n",
    "    counts[word] = counts.get(word, 0) + 1\n",
    "\n",
    "result = {word: count for word, count in counts.items() if count > 1}\n",
    "\n",
    "print(\"Output:\", result)"
   ]
