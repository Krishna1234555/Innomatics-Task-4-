   "source": [
    "sensor_readings = [3, 4, 7, 8, 10, 12, 5]\n",
    "\n",
    "valid_readings = []\n",
    "\n",
    "for hour in range(len(sensor_readings)):\n",
    "    value = sensor_readings[hour]\n",
    "    \n",
    "    if value % 2 == 0:\n",
    "        valid_readings.append((hour, value))\n",
    "\n",
    "print(\"Valid Sensor Readings (Hour, Value):\", valid_readings)"
   ]
